{
    "manifest_version": 3,
    "name": "My meowcho",
    "version": "1.0",
    "description": "Adds an interactive cat to your tab with a progress tracker.",
    "permissions": ["storage"],
    "host_permissions": ["<all_urls>"],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"],
        "css": ["style.css"]
      }
    ],
    "icons": {
      "48": "assets/cat-default.png",
      "128": "assets/cat-default.png"
    }
  }
  